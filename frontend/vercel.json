{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "rewrites": [
      {
        "source": "/api/auth/:path*",
        "destination": "/api/auth/:path*"
      },
      {
        "source": "/admin/:path*",
        "destination": "/admin/:path*"
      }
    ]
  }
  